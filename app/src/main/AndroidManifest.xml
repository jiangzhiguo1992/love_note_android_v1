        <activity android:name="com.jiangzg.ita.activity.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <!-- 主页面 -->
        <activity
            android:name="com.jiangzg.ita.activity.HomeActivity"
            android:launchMode="singleTask"/>
        <!-- 更新服务 -->
        <service
            android:name="com.jiangzg.ita.service.UpdateService"
