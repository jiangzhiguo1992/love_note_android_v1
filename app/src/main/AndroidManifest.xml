<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.jiangzg.mianmian">

    <!-- 蓝牙信息进行室内定位 -->
    <uses-permission android:name="android.permission.BLUETOOTH"/>
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>

    <application
        android:name=".base.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppThemeGrey">

        <!-- 高德地图 -->
        <meta-data
            android:name="com.amap.api.v2.apikey"
            android:value="8d050bd3da20cc18f5f066710b8b8974"/>

        <service
            android:name="com.amap.api.location.APSService"
            android:enabled="true"
            android:exported="true"/>

        <!-- data-下载渠道 -->
        <meta-data
            android:name="market_channel"
            android:value="${MARKET_CHANNEL_VALUE}"/>

        <!-- service-apk更新 -->
        <service
            android:name=".service.UpdateService"
            android:enabled="true"
            android:exported="true"/>
        <!-- service-定时器 -->
        <service
            android:name=".service.TimerService"
            android:enabled="true"
            android:exported="true">
        </service>
        <!-- receiver-oss -->
        <receiver
            android:name=".broadcast.OssRefreshReceiver"
            android:enabled="true"
            android:exported="true">
        </receiver>

        <!-- activity -->
        <activity android:name=".activity.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.HomeActivity"/>
        <activity android:name=".activity.user.LoginActivity"/>
        <activity android:name=".activity.user.RegisterActivity"/>
        <activity android:name=".activity.couple.CoupleInfoActivity"/>
        <activity android:name=".activity.user.ForgetActivity"/>
        <activity android:name=".activity.common.WebActivity"/>
        <activity android:name=".activity.user.UserInfoActivity"/>
        <activity android:name=".activity.settings.HelpActivity"/>
        <activity android:name=".activity.common.SettingsActivity"/>
        <activity android:name=".activity.settings.ThemeActivity"/>
        <activity android:name=".activity.user.PhoneActivity"/>
        <activity android:name=".activity.user.PasswordActivity"/>
        <activity android:name=".activity.settings.SuggestHomeActivity"/>
        <activity android:name=".activity.settings.SuggestDetailActivity"/>
        <activity android:name=".activity.settings.SuggestAddActivity"/>
        <activity android:name=".activity.settings.SuggestListActivity"/>
        <activity android:name=".activity.couple.CoupleWallPaperActivity"/>
        <activity android:name=".activity.couple.CouplePairActivity"/>
        <activity android:name=".activity.book.WordListActivity"/>
        <activity android:name=".activity.book.DiaryListActivity"/>
        <activity android:name=".activity.book.DiaryEditActivity"/>
        <activity android:name=".activity.book.DiaryDetailActivity"/>
        <activity android:name=".activity.book.WhisperListActivity"/>
        <activity android:name=".activity.common.BigImageActivity"/>
        <activity android:name=".activity.book.AlbumListActivity"/>
        <activity android:name=".activity.book.AlbumEditActivity"/>
        <activity android:name=".activity.book.PictureListActivity"/>
        <activity android:name=".activity.couple.CouplePlaceActivity"/>
        <activity android:name=".activity.couple.CoupleWeatherActivity"/>
        <activity android:name=".activity.common.MapShowActivity"/>
        <activity android:name=".activity.settings.AboutActivity"/>
        <activity android:name=".activity.settings.NoticeListActivity"/>
        <activity android:name=".activity.settings.NoticeDetailActivity"/>
        <activity android:name=".activity.book.PictureEditActivity"/>
        <activity android:name=".activity.common.MapSelectActivity"/>
        <activity android:name=".activity.common.MapSearchActivity"/>
        <activity android:name=".activity.book.DreamListActivity"/>
        <activity android:name=".activity.book.DreamEditActivity"/>
        <activity android:name=".activity.book.DreamDetailActivity"/>
        <activity android:name=".activity.book.GiftListActivity"/>
        <activity android:name=".activity.book.GiftEditActivity"/>
        <activity android:name=".activity.book.PromiseListActivity"/>
        <activity android:name=".activity.book.PromiseEditActivity"/>
        <activity android:name=".activity.book.PromiseDetailActivity"/>
    </application>

</manifest>