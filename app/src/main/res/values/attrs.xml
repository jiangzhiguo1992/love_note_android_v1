<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- 记得在xml引用时加命名空间 xmlns:my="http://schemas.android.com/apk/res-auto"-->
    <declare-styleable name="GZoomScrollView">
        <attr name="custom_layout_index" format="integer"/>
    </declare-styleable>

    <declare-styleable name="GNumberPicker">
        <attr name="custom_divider_color" format="color"/>
        <attr name="custom_text_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="GImageView">
        <attr name="is_net_circle" format="boolean"/>
        <attr name="is_net_full" format="boolean"/>
        <attr name="is_net_normal" format="boolean"/>
    </declare-styleable>

</resources>